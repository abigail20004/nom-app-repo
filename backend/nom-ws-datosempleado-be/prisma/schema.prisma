generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model colaborador {
  idcolaborador Int @id @default(autoincrement())
  nombre         String
  apellido       String
  direccion      String
  edad           Int
  profesion      String
  estado_civil   String
}
